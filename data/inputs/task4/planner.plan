(click_navigate t_contacts_icon home s_contacts)
(scroll down s_contacts s_contacts_list)
(click_navigate t_alice_entry s_contacts_list s_contact_alice)
(click_navigate t_message_button s_contact_alice s_message_editor)

;; --- MCP pipeline ---
(mcp_call tool_llm par_generate_reply d_reply_text)
(make_text_ready_from_deliverable d_reply_text txt_auto_reply)

;; --- UI input ---
(click_focus_field t_body_field s_message_editor f_body)
(input_text f_body txt_auto_reply)

;; --- send ---
