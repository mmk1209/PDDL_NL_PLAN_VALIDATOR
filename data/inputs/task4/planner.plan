;; --- return home ---
(navigate_back s_sent s_message_editor)
(navigate_back s_message_editor s_contact_alice)
(navigate_back s_contact_alice s_contacts_list)
(navigate_back s_contacts_list s_contacts)
(navigate_back s_contacts home)

;; --- finalize ---
(status complete)
